<div style="column-width: 14em;">
<ul>
{{- range .Services }}
  {{- if .Aliases }}
  <li><code>{{ .ProviderPackage }}</code> ({{ range $i, $e := .Aliases }}{{ if gt $i 0 }} {{ end }}or <code>{{ $e }}</code>{{ end }})</li>
  {{- else }}
  <li><code>{{ .ProviderPackage }}</code></li>
  {{- end }}
{{- end }}
</ul>
</div>
